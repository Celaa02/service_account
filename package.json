{
  "name": "bank-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "build": "nest build",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "test": "jest --coverage",
    "test:unit": "jest --selectProjects unit",
    "test:e2e": "jest --selectProjects e2e",
    "test:cov": "jest --coverage",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/infra/config/typeorm.datasource.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/infra/config/typeorm.datasource.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/infra/db/typeorm/migrations/init -d src/infra/config/typeorm.datasource.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "ajv": "6.12.6",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "cosmiconfig": "^9.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "@eslint/eslintrc@3.3.1": {
      "ajv": "6.12.6"
    }
  }
}
